#!/bin/bash

LOG_FILE="/var/www/html/vpn-web-installer/update.log"

echo "$(date) - Проверка обновлений началась..." >> $LOG_FILE

# Логирование процесса
if [ $? -eq 0 ]; then
    echo "$(date) - Обновление успешно завершено." >> $LOG_FILE
else
    echo "$(date) - Ошибка обновления, откат." >> $LOG_FILE
fi
